{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all"
  },
  "productName": "Progress Tracker",
  "mainBinaryName": "Progress Tracker",
  "version": "1.2.2",
  "identifier": "com.h8m0ss.progress-tracker",
  "plugins": {
    "cli": {
      "description": "",
      "longDescription": "",
      "beforeHelp": "",
      "afterHelp": "",
      "args": [
        {
          "name": "file",
          "index": 1,
          "takesValue": true
        }
      ]
    }
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Progress Tracker",
        "width": 800,
        "height": 600,
        "useHttpsScheme": true
      }
    ],
    "withGlobalTauri": false,
    "security": {
      "csp": null
    }
  }
}